<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.supremebuildswizard" name="Supreme Builds Wizard" version="4.0.1" provider-name="KodiSkills">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Supreme Builds Wizard</summary>
    <description lang="en">The Supreme Builds Wizard contains all official Supreme Builds, Add-ons, Apps, Maintenance Tools, Settings and more.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.supremebuilds" name="Supreme Builds Repository" version="1.0.1" provider-name="KodiSkills">
	<extension point="xbmc.addon.repository" name="Supreme Builds Repository">
		<info compressed="false">https://raw.githubusercontent.com/kodiskills/supremebuildsrepo/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/kodiskills/supremebuildsrepo/master/addons.xml.md5</checksum>
		<datadir zip="true">https://github.com/kodiskills/supremebuildsrepo/raw/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Supreme Builds Repository</summary>
		<description>Official Repository Of The Supreme Builds Team</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
